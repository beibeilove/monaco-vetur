{
  "name": "monaco-vue",
  "version": "0.2.2",
  "description": "Vue plugin for the Monaco Editor",
  "scripts": {
    "compile": "gulp compile",
    "watch": "gulp watch",
    "prepublish": "gulp release",
    "install-service-next": "npm install vscode--languageservice@next -f -D && npm install vscode-languageserver-types@next -f -D",
    "install-service-local": "npm install ../vscode-html-languageservice -f -D && npm install ../vscode-languageserver-node/types -f -D"
  },
  "author": "rebornix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rebornix/monaco-vue"
  },
  "bugs": {
    "url": "https://github.com/rebornix/monaco-vue/issues"
  },
  "devDependencies": {
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-requirejs": "^1.3.0",
    "gulp-tsb": "^4.0.5",
    "gulp-uglify": "^3.0.2",
    "http-server": "^0.11.1",
    "merge-stream": "^2.0.0",
    "monaco-editor-core": "^0.18.1",
    "monaco-languages": "^1.8.0",
    "monaco-typescript": "^3.5.1",
    "object-assign": "^4.1.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2",
    "vscode-html-languageservice": "^3.0.3",
    "vscode-languageserver-types": "^3.14.0",
    "vscode-css-languageservice": "^4.0.2"
